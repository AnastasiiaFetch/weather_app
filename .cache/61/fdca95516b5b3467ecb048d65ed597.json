d-color: var(--color-black);\r\n}\r\n\r\nmain > section:first-of-type {\r\n  width: 100%;\r\n  height: 20vh;\r\n  padding: 2rem 0;\r\n  display: flex;\r\n  justify-content: space-around;\r\n}\r\n\r\nmain > section:first-of-type > div {\r\n  font-size: 1.2rem;\r\n  align-self: center;\r\n  cursor: pointer;\r\n}\r\n\r\nmain > section:last-child {\r\n  display: none;\r\n  width: 100%;\r\n  justify-content: space-evenly;\r\n  margin-bottom: 2rem;\r\n}\r\n\r\nform {\r\n  width: 70%;\r\n  display: flex;\r\n  align-items: center;\r\n  justify-content: center;\r\n}\r\n\r\nform > input {\r\n  width: 60%;\r\n  padding: 0.5rem;\r\n  border-radius: 1rem;\r\n  border: none;\r\n  text-align: center;\r\n}\r\n\r\nform > input:focus{\r\n  outline: none;\r\n}\r\n\r\nform > button {\r\n  width: 20%;\r\n  padding: 0.6rem;\r\n  margin-left: 1rem;\r\n  border-radius: 1rem;\r\n  border: none;\r\n  background-color: var(--color-gray);\r\n  color: var(--color-white);\r\n  cursor: pointer;\r\n}\r\n\r\nform > button:hover {\r\n  box-shadow: 5px 3px 10px 3px rgba(0, 0, 0, 0.5);\r\n}\r\n\r\n.main_page_wrapper {\r\n  display: flex;\r\n  flex-direction: column;\r\n  align-items: center;\r\n  justify-content: center;\r\n  text-align: center;\r\n  position: absolute;\r\n  top: 50%;\r\n  left: 50%;\r\n  transform: translate(-50%, -50%);\r\n}\r\n\r\n.main_page_wrapper > div:last-child {\r\n  align-self: center;\r\n  font-size: 2rem;\r\n}\r\n\r\n.outputForecast {\r\n  width: 50%;\r\n}\r\n\r\n.outputInfo {\r\n  width: 40%;\r\n  display: flex;\r\n  flex-direction: column;\r\n  align-self: flex-start;\r\n}\r\n\r\n.main_info_wrapper {\r\n  display: flex;\r\n  flex-direction: row;\r\n  justify-content: space-between;\r\n  padding: 0.5rem;\r\n}\r\n\r\n.location_info_wrapper {\r\n  font-size: 1.2rem;\r\n  align-self: center;\r\n}\r\n\r\nspan {\r\n  font-weight: 700;\r\n}\r\n\r\n.condition_desc {\r\n  font-size: 1.2rem;\r\n}\r\n\r\n.condition_info_wrapper {\r\n  text-align: center;\r\n  display: flex;\r\n  align-items: center;\r\n}\r\n\r\n.detailed_info_wrapper {\r\n  border-top: 1px solid var(--color-gray);\r\n  padding: 0.5rem;\r\n  font-size: 1.2rem;\r\n}\r\n\r\n.forecast_card {\r\n  display: flex;\r\n  justify-content: space-evenly;\r\n  align-items: center;\r\n  border-radius: 1rem;\r\n}\r\n\r\n.forecast_card:nth-child(2n) {\r\n  background-color: var(--color-gray);\r\n}\r\n\r\n.forecast_card:nth-child(2n + 1) {\r\n  border: 1px solid var(--color-gray);\r\n}\r\n\r\n.forecast_card > div {\r\n  width: 20%;\r\n  text-align: center;\r\n}\r\n\r\n.forecast_section_header {\r\n  font-size: 1rem;\r\n  display: flex;\r\n  justify-content: space-evenly;\r\n  background-color: var(--color-gray);\r\n  margin-bottom: 1rem;\r\n  padding: 1rem;\r\n  border-radius: 1rem;\r\n}\r\n\r\n.forecast_section_cards {\r\n  width: 100%;\r\n}\r\n\r\n/* scrollbar */\r\n\r\n::-webkit-scrollbar {\r\n  width: 8px;\r\n}\r\n\r\n::-webkit-scrollbar-thumb {\r\n  background-color: var(--color-gray);\r\n  border-radius: 5px;\r\n}\r\n\r\n::-webkit-scrollbar-track {\r\n  background-color: var(--color-black);\r\n}\r\n\r\n@media (max-width: 768px) {\r\n\r\n  html {\r\n    font-size: 10px;\r\n  }\r\n\r\n  main{\r\n    justify-content: normal;\r\n  }\r\n\r\n  main > section:first-of-type {\r\n    flex-direction: column;\r\n    height: fit-content;\r\n    justify-content: space-around;\r\n  }\r\n\r\n  main > section:first-of-type > div {\r\n    margin-bottom: 1rem;\r\n  }\r\n\r\n  main > section:last-child {\r\n    flex-direction: column;\r\n    justify-content: flex-start;\r\n    width: 100%;\r\n  }\r\n\r\n  form {\r\n    width: 100%;\r\n  }\r\n\r\n  .main_page_wrapper > div:last-child {\r\n    align-self: center;\r\n    font-size: 1rem;\r\n  }\r\n\r\n  .outputForecast {\r\n    width: 100%;\r\n  }\r\n\r\n  .outputInfo {\r\n    width: 100%;\r\n    flex-direction: row;\r\n    justify-content: space-evenly;\r\n    margin: 1rem 0;\r\n    border-top: 1px solid var(--color-gray);\r\n    border-bottom: 1px solid va